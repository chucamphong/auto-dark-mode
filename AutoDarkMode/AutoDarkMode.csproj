<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <Nullable>enable</Nullable>
        <ApplicationIcon />
        <StartupObject>AutoDarkMode.Program</StartupObject>
        <Authors>Chu Phong &lt;chucamphong1999@gmail.com&gt;</Authors>
        <Description>Tự động chuyển đổi giao diện sáng tối theo thời gian thực.</Description>
        <Copyright>Chu Phong &lt;chucamphong1999@gmail.com&gt;</Copyright>
    </PropertyGroup>

    <ItemGroup>
      <None Include="..\.editorconfig" Link=".editorconfig" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Win32.Registry" Version="4.7.0" />
      <PackageReference Include="StyleCop.Analyzers" Version="1.1.118">
        <PrivateAssets>all</PrivateAssets>
        <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      </PackageReference>
      <PackageReference Include="TaskScheduler" Version="2.8.18" />
    </ItemGroup>

</Project>
